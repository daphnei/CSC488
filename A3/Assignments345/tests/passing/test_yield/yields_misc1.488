begin

    integer result
    
    integer dog
	dog <= 1
    result <= { integer cat cat <= dog yields cat }
	put result, skip
    result <= { integer rat if dog = 2 then rat <= 1 else rat <= 5 end yields rat }
    put result, skip
    if {
        integer cheese
        cheese <= 6
        while cheese > 0
        do
            cheese <= cheese - 1
        end
        yields
        cheese + 1
    } > 10 then
        dog <= 2
    end
end
