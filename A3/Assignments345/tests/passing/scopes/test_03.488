% Accessing identifiers declared in main scope from inside routines
% S00, S01, S03, S21, S47, S10, S37, S41, S08, S14, S16, S15, S18, S09, S13
% S34, S26, S31, S20, S51, S35

begin

	integer outx, outy

	procedure p (integer x, integer y)
	begin
		outx <= 5
		outy <= 15
		put outx, outy
	end

	boolean function f
	begin
		outx <= outx + 1
		outy <= outx + outy
		return (outy <= 10)
	end

end