begin 

	integer globalNum
	globalNum <= 0
	
	procedure print100 begin
	
		put globalNum
		globalNum <= globalNum + 1
		if globalNum <= 100 then
			print100
		end
	end




	integer function fatorial (integer n) begin
		if n = 0 then
			return (1)
		else
			return (n * fatorial(n-1)) 
		end
	end


	print100
	fatorial(globalNum)

end
